function net = weiTOnet(wei)
%% This operation transforms the weight vector to the network.
net = InitNet(0); % create empty network
N = length(net.layers);
id = 0;
if mod(length(wei),2) ~= 0
    print('number of weights must be multiple of 2')
end
L = length(wei)/2;

for n = 1:N
    l = net.layers{n};
    if isfield(l, 'weights')
        for i = 1:length(l.weights)
            idp = id + numel(l.weights{i});
            idp2 = id + L + numel(l.weights{i});
            weitemp = wei(id+1:idp) + 1i*wei(L + id + 1+:idp2);
            net.layers{n}.weights{i} = reshape(weitemp, size(l.weights{i}));
            id = idp;
        end
    end
end
end